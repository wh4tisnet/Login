<Window x:Class="Test1.accountmanagemnt"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Test1"
        mc:Ignorable="d"
        Title="accountmanagemnt" Height="450" Width="800" Loaded="Window_Loaded" Background="LightGray">

    <Grid>
        <TextBlock Text="Name:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="20,20,0,0" FontSize="14" Foreground="Black" />
        <TextBox Name="NameTextBox" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="135,20,0,0" Width="200" Height="25" Foreground="Black"/>
        <TextBlock Text="Password:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="20,60,0,0" FontSize="14" Foreground="Black"/>
        <PasswordBox Name="PasswordBox" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="135,57,0,0" Width="200" Height="25" Foreground="Black"/>
        <TextBlock Text="Email:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="20,100,0,0" FontSize="14" Foreground="Black"/>
        <TextBox Name="EmailTextBox" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="135,97,0,0" Width="200" Height="25" Foreground="Black"/>
        <TextBlock Text="Live:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="20,140,0,0" FontSize="14" Foreground="Black"/>
        <TextBox Name="LiveTextBox" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="135,134,0,0" Width="200" Height="25" Foreground="Black"/>
        <TextBlock Text="Videogame:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="20,180,0,0" FontSize="14" Foreground="Black"/>
        <TextBox Name="VideogameTextBox" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="135,174,0,0" Width="200" Height="25" Foreground="Black"/>
        <TextBlock Text="Phone:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="20,220,0,0" FontSize="14" Foreground="Black"/>
        <TextBox Name="PhoneTextBox" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="135,220,0,0" Width="200" Height="25" Foreground="Black"/>
        <TextBlock Text="Address:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="20,260,0,0" FontSize="14" Foreground="Black"/>
        <TextBox Name="AddressTextBox" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="135,260,0,0" Width="200" Height="25" Foreground="Black"/>
        <TextBlock Text="Country:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="20,300,0,0" FontSize="14" Foreground="Black"/>
        <TextBox Name="CountryTextBox" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="135,300,0,0" Width="200" Height="25" Foreground="Black"/>
        <TextBlock Text="Age:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="20,340,0,0" FontSize="14" Foreground="Black"/>
        <TextBox Name="AgeTextBox" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="135,340,0,0" Width="200" Height="25" Foreground="Black"/>
        <TextBlock Text="Favorite Movie:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="20,380,0,0" FontSize="14" Foreground="Black"/>
        <TextBox Name="FavoriteMovieTextBox" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="135,377,0,0" Width="200" Height="25" Foreground="Black"/>
        <Button Name = "Saveas" Content="Save as the information" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="616,369,0,0" Width="154" Height="30" Click="Saveas_Click" Foreground="Black" Background="LightBlue"/>
        <TextBox x:Name="IdTextBox" IsReadOnly="True" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="718,17,0,0" Width="72" Height="25" Foreground="Black"/>
        <TextBlock Text="Id:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="698,20,0,0" FontSize="14" Foreground="Black" />
        <TextBlock Text="User:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="466,133,0,0" FontSize="14" Foreground="Black"/>
        <TextBox Name="UsersTextBox" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="506,134,0,0" Width="200" Height="25" Foreground="Black"/>
        <Button Content="Open User Management Console" Click="OpenPart2_Click" VerticalAlignment="Top" Margin="547,180,46,0" Height="66"/>
    </Grid>
</Window>